    while task.wait(0.1) do
        if CFG.InfStamina and P:FindFirstChild("Energy") then
            P.Energy.Value = math.huge
        end
    end
end)

task.spawn(function()
    while true do
        task.wait(CurrentSpeed)
        if not CFG.KillAura then continue end
        local char = P.Character
        if not char or not char:FindFirstChild("HumanoidRootPart") then continue end
        if char.Humanoid.Health <= 0 then continue end
        local root = char.HumanoidRootPart
        local closest, dist = nil, math.huge
        for _, e in workspace.Characters.Enemies:GetChildren() do
            local hum = e:FindFirstChild("Humanoid")
            local eroot = e:FindFirstChild("HumanoidRootPart")
            if hum and eroot and hum.Health > 0 then
                local d = (root.Position - eroot.Position).Magnitude
                if d < dist then
                    dist = d
                    closest = {Humanoid = hum, Root = eroot}
                end
            end
        end
        if closest then
            local pos = closest.Root.Position + Vector3.new(0,30,0)
            pcall(function()
                Charge:FireServer(YOUR_CLASS, {Mousepos = pos})
                Activate:FireServer(YOUR_CLASS, {Charge=100, Mousepos=pos, ComboNum=1}, 1, "PaintballBlast")
                Damage:FireServer(closest.Humanoid, YOUR_CLASS)
            end)
        end
    end
end)

print("SBBF v9.5 Premium UI loaded (Disruptor)")
]]

    -- ==================== Paintball script (the full version we built) ====================
    local paintballCode = [[
-- SBBF v9.5 - Paintball (Legit Kill Aura + Normal Kill Aura - NO TELEPORT)
-- Aim Assist on Shift Lock

local Players = game:GetService("Players")
local ReplicatedStorage = game:GetService("ReplicatedStorage")
