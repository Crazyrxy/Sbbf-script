    label.Size = UDim2.new(0.62,0,1,0)
    label.BackgroundTransparency = 1
    label.Text = text
    label.Font = Enum.Font.GothamSemibold
    label.TextSize = 16
    label.TextColor3 = Color3.fromRGB(235,235,245)
    label.TextXAlignment = Enum.TextXAlignment.Left
    label.Position = UDim2.new(0.04,0,0,0)
    
    local toggle = Instance.new("TextButton", holder)
    toggle.Size = UDim2.new(0, 82, 0, 32)
    toggle.Position = UDim2.new(1, -95, 0.5, -16)
    toggle.Font = Enum.Font.GothamBold
    toggle.TextSize = 15
    toggle.TextColor3 = Color3.new(1,1,1)
    local tc = Instance.new("UICorner", toggle)
    tc.CornerRadius = UDim.new(1,0)
    
    local state = default
    local function update()
        toggle.Text = state and "ON" or "OFF"
        toggle.BackgroundColor3 = state and Color3.fromRGB(0, 205, 120) or Color3.fromRGB(75,75,85)
    end
    
    toggle.MouseButton1Click:Connect(function()
        state = not state
        update()
        callback(state)
    end)
    update()
    
    yPos = yPos + 58
end

local function createSpeedInput()
    local holder = Instance.new("Frame", main)
    holder.Size = UDim2.new(0.92, 0, 0, 110)
    holder.Position = UDim2.new(0.04, 0, 0, yPos)
    holder.BackgroundColor3 = Color3.fromRGB(28,28,38)
    holder.BorderSizePixel = 0
    local hc = Instance.new("UICorner", holder)
    hc.CornerRadius = UDim.new(0,10)
    
    local toggle = Instance.new("TextButton", holder)
    toggle.Size = UDim2.new(0, 82, 0, 32)
    toggle.Position = UDim2.new(0.04, 0, 0, 12)
    toggle.Font = Enum.Font.GothamBold
    toggle.TextSize = 15
    toggle.TextColor3 = Color3.new(1,1,1)
    toggle.BackgroundColor3 = Color3.fromRGB(75,75,85)
    toggle.Text = "OFF"
    local tc = Instance.new("UICorner", toggle)
    tc.CornerRadius = UDim.new(1,0)
    
    local label = Instance.new("TextLabel", holder)
    label.Size = UDim2.new(0.5,0,0,32)
    label.Position = UDim2.new(0.28, 0, 0, 12)
    label.BackgroundTransparency = 1
    label.Text = "Custom Speed (only when ON)"
    label.Font = Enum.Font.GothamSemibold
    label.TextSize = 16
    label.TextColor3 = Color3.fromRGB(235,235,245)
    label.TextXAlignment = Enum.TextXAlignment.Left
    
    local textbox = Instance.new("TextBox", holder)
    textbox.Size = UDim2.new(0.92, 0, 0, 36)
    textbox.Position = UDim2.new(0.04, 0, 0, 55)
    textbox.BackgroundColor3 = Color3.fromRGB(40,40,55)
    textbox.TextColor3 = Color3.fromRGB(200,220,255)
    textbox.PlaceholderText = "Enter speed (e.g. 80)"
    textbox.Text = tostring(CFG.CustomSpeedValue)
    textbox.Font = Enum.Font.Gotham
    textbox.TextSize = 16
    textbox.ClearTextOnFocus = false
    local tbc = Instance.new("UICorner", textbox)
    tbc.CornerRadius = UDim.new(0,8)
    
    local state = CFG.CustomSpeedEnabled
    local function updateToggle()
        toggle.Text = state and "ON" or "OFF"
        toggle.BackgroundColor3 = state and Color3.fromRGB(0, 205, 120) or Color3.fromRGB(75,75,85)
        CFG.CustomSpeedEnabled = state
    end
    
    toggle.MouseButton1Click:Connect(function()
        state = not state
        updateToggle()
    end)
    updateToggle()
    
    local function applySpeed()
        if not CFG.CustomSpeedEnabled then return end
        local num = tonumber(textbox.Text)
        if num and num > 0 and Player.Character then
            CFG.CustomSpeedValue = num
            Player.Character.Humanoid.WalkSpeed = num
        end
    end
    
    textbox.FocusLost:Connect(applySpeed)
    textbox.MouseLeave:Connect(applySpeed)
    
    yPos = yPos + 120
end

createToggle("Legit Kill Aura (4-9s, 50% miss)", true, function(v) 
    CFG.LegitKillAura = v
    if v then CFG.NormalKillAura = false end
end)
createToggle("Normal Kill Aura (Fast - NO TELEPORT)", false, function(v) 
    CFG.NormalKillAura = v
    if v then CFG.LegitKillAura = false end
end)
createToggle("Infinite Stamina (Shows 100)", true, function(v) CFG.InfStamina = v end)
createToggle("Aim Assist (Shift Lock)", true, function(v) CFG.AimAssist = v end)
createSpeedInput()

-- AIM ASSIST (SHIFT LOCK)
local AimTarget = nil

local function FindBestTarget()
    local char = Player.Character
    if not char or not char:FindFirstChild("HumanoidRootPart") then return nil end
    local hrp = char.HumanoidRootPart
    
    local targets = {}
    for _, enemy in Workspace.Characters.Enemies:GetChildren() do
        local hum = enemy:FindFirstChild("Humanoid")
        if not hum or hum.Health <= 0 then continue end
        local root = enemy:FindFirstChild("HumanoidRootPart")
        if not root then continue end
        local part = enemy:FindFirstChild(CFG.AimPart) or root
        local dist = (hrp.Position - root.Position).Magnitude
        table.insert(targets, {
            Model = enemy,
            Root = root,
            AimPart = part,
            Distance = dist,
            IsBoss = enemy.Name:match("Boss") or enemy.Name:match("Elite") or enemy.Name:match("King")
        })
    end
    
    if #targets == 0 then return nil end
    
    table.sort(targets, function(a,b)
        if a.IsBoss and not b.IsBoss then return true end
        if not a.IsBoss and b.IsBoss then return false end
        return a.Distance < b.Distance
    end)
    
    return targets[1]
end

local ShiftLockEnabled = false
UserInputService.InputBegan:Connect(function(input, gpe)
    if gpe then return end
    if input.KeyCode == Enum.KeyCode.LeftShift then
        ShiftLockEnabled = true
        AimTarget = FindBestTarget()
    end
end)

UserInputService.InputEnded:Connect(function(input, gpe)
    if gpe then return end
    if input.KeyCode == Enum.KeyCode.LeftShift then
        ShiftLockEnabled = false
        AimTarget = nil
    end
end)

RunService.RenderStepped:Connect(function()
    if not CFG.AimAssist then 
        AimTarget = nil
        return 
    end
    
    local isShiftLock = UserInputService.MouseBehavior == Enum.MouseBehavior.LockCenter
    if not isShiftLock then 
        AimTarget = nil
        return 
    end
    
    if not AimTarget or not AimTarget.AimPart or not AimTarget.AimPart.Parent then
        AimTarget = FindBestTarget()
        if not AimTarget then return end
    end
    
    local aimPos = AimTarget.AimPart.Position
    local currentCFrame = Camera.CFrame
    local currentLook = currentCFrame.LookVector
    local targetDir = (aimPos - currentCFrame.Position).Unit
    
    local newLook = currentLook:Lerp(targetDir, CFG.AimSmoothness)
    newLook = newLook.Unit
    
    Camera.CFrame = CFrame.new(currentCFrame.Position, currentCFrame.Position + newLook)
end)

-- LEGIT KILL AURA
task.spawn(function()
    while task.wait(math.random(4,9)) do
        if not CFG.LegitKillAura then continue end
        
        local char = Player.Character
        if not char or not char:FindFirstChild("HumanoidRootPart") then continue end
        
        local targets = {}
        for _, enemy in Workspace.Characters.Enemies:GetChildren() do
            local hum = enemy:FindFirstChild("Humanoid")
            if not hum or hum.Health <= 0 then continue end
            local root = enemy:FindFirstChild("HumanoidRootPart")
            if not root then continue end
            local dist = (char.HumanoidRootPart.Position - root.Position).Magnitude
            table.insert(targets, {
                Enemy = enemy, Humanoid = hum, Root = root,
                Distance = dist,
                IsBoss = enemy.Name:match("Boss") or enemy.Name:match("Elite") or enemy.Name:match("King")
            })
        end
        
        if #targets == 0 then continue end
        
        table.sort(targets, function(a,b)
            if a.IsBoss and not b.IsBoss then return false end
            if not a.IsBoss and b.IsBoss then return true end
            return a.Distance < b.Distance
        end)
        
        local best = targets[1]
        local eRoot = best.Root
        
        local isMiss = math.random() < 0.5
        local offset = Vector3.new(
            isMiss and math.random(-60,60) or math.random(-12,12),
            math.random(30,50),
            isMiss and math.random(-60,60) or math.random(-12,12)
        )
        local targetPos = eRoot.Position + offset
        
        pcall(function() Charge:FireServer(YOUR_CLASS, {Mousepos = targetPos}) end)
        task.wait(0.07)
        pcall(function() Activate:FireServer(YOUR_CLASS, {Charge=100, Mousepos=targetPos, ComboNum=1}, 1, "PaintballBlast") end)
        task.wait(0.07)
        pcall(function() Damage:FireServer(best.Humanoid, YOUR_CLASS) end)
    end
end)

-- NORMAL KILL AURA
task.spawn(function()
    while task.wait(0.24) do
        if not CFG.NormalKillAura then continue end
        local char = Player.Character
        if not char or not char:FindFirstChild("HumanoidRootPart") then continue end
        local hrp = char.HumanoidRootPart
        local targets = {}
        
        for _, enemy in Workspace.Characters.Enemies:GetChildren() do
            local hum = enemy:FindFirstChild("Humanoid")
            if not hum or hum.Health <= 0 then continue end
            local root = enemy:FindFirstChild("HumanoidRootPart")
            if not root then continue end
            local dist = (hrp.Position - root.Position).Magnitude
            table.insert(targets, {
                Enemy = enemy, Humanoid = hum, Root = root,
                Distance = dist,
                IsBoss = enemy.Name:match("Boss") or enemy.Name:match("Elite") or enemy.Name:match("King")
            })
        end
        
        if #targets == 0 then continue end
        
        table.sort(targets, function(a,b)
            if a.IsBoss and not b.IsBoss then return false end
            if not a.IsBoss and b.IsBoss then return true end
            return a.Distance < b.Distance
        end)
        
        local best = targets[1]
        local eRoot = best.Root
        
        local offset = Vector3.new(math.random(-14,14), math.random(34,52), math.random(-14,14))
        local targetPos = eRoot.Position + offset
        
        pcall(function() Charge:FireServer(YOUR_CLASS, {Mousepos = targetPos}) end)
        task.wait(0.07)
        pcall(function() Activate:FireServer(YOUR_CLASS, {Charge=100, Mousepos = targetPos, ComboNum=1}, 1, "PaintballBlast") end)
        task.wait(0.07)
        pcall(function() Damage:FireServer(best.Humanoid, YOUR_CLASS) end)
    end
end)

-- Infinite Stamina
task.spawn(function()
    while task.wait(0.1) do
        if CFG.InfStamina then
            if Player:FindFirstChild("Energy") then Player.Energy.Value = 100 end
            local char = Player.Character
            if char and char:FindFirstChild("Humanoid") then
                local hum = char.Humanoid
                if hum:GetAttribute("Stamina") then hum:SetAttribute("Stamina", 100) end
            end
        end
    end
end)

-- Speed keeper
task.spawn(function()
    while task.wait(0.3) do
        if CFG.CustomSpeedEnabled and Player.Character and Player.Character:FindFirstChild("Humanoid") then
            Player.Character.Humanoid.WalkSpeed = CFG.CustomSpeedValue
        end
    end
end)

print("SBBF v9.5 ‚Äì Aim Assist on SHIFT LOCK loaded")
]]

    -- ==================== Load functions ====================

    btnDisruptor.MouseButton1Click:Connect(function()
        sg:Destroy()
        print("Launching Disruptor...")
        loadstring(disruptorCode)()
    end)

    btnPaintball.MouseButton1Click:Connect(function()
        sg:Destroy()
        print("Launching Paintball...")
        loadstring(paintballCode)()
    end)
end)

if not success then
    warn("Launcher failed: " .. tostring(err))
end

print("Launcher ready - pick Disruptor or Paintball")
]]

loadstring(code)()
