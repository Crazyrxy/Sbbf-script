local Damage = Remotes:WaitForChild("Damage")
local YOUR_CLASS = "Paintball"

local CFG = {
    NormalKillAura = false,
    LegitKillAura = true,
    InfStamina = true,
    CustomSpeedEnabled = false,
    CustomSpeedValue = 80,
    AimAssist = true,
